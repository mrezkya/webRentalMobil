  <!-- ======= Sidebar ======= -->

  <main id="main" class="main">

    <div class="pagetitle">
      <h1>Form Elements</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="index.html">Home</a></li>
          <li class="breadcrumb-item">Forms</li>
          <li class="breadcrumb-item active">Elements</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->

    <section class="section">
      <div class="row">
        <div class="col-lg-6">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Advanced Form Elements</h5>

              <!-- Advanced Form Elements -->
              <form>
                <% result.forEach((item) => { %>
                <div class="row mb-5">
                  <p name="idku" id="oke"><%= item.id %></p>
                  <img src="/images/<%= item.image %>" class="rounded mx-auto d-block" alt="...">
                  <b>nama kendaraan : <%= item.nama_kendaraan %> </b>
                  <b>merk kendaraan : <%= item.merek_kendaraan %></b>
                  <b>kursi : <%= item.seats %></b>
                  <b name="harga" id="harga">harga : <%= item.price %></b>
                  <b>engine : <%= item.engine %></b>
                  <b>type : <%= item.engine %></b>
                </div>
                <% }) %>
              </form><!-- End General Form Elements -->

            </div>
          </div>

        </div>

        <div class="col-lg-6">

          <div class="card">
            <div class="card-body">
              <h5 class="card-title">General Form Elements</h5>

              <!-- General Form Elements -->
                <form method="post" action="/bookP ">
                <div class="row mb-3">
                  <label for="inputText" class="col-sm-2 col-form-label">NIK</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="nik">
                  </div>
                </div>
                <div class="row mb-3">
                  <label for="inputText" class="col-sm-2 col-form-label">Nama</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="nama">
                  </div>
                </div>
                <div class="row mb-3">
                  <label for="inputEmail" class="col-sm-2 col-form-label">kontak/wa</label>
                  <div class="col-sm-10">
                    <input type="text" class="form-control" name="kontak">
                  </div>
                </div>
                <fieldset class="row mb-3">
                  <legend class="col-form-label col-sm-2 pt-0">Opsi</legend>
                  <div class="col-sm-10">
                    <div class="form-check">
                      <input class="form-check-input" type="radio" name="gridRadios" id="gridRadios1" value="sendiri" checked>
                      <label class="form-check-label" for="gridRadios1">
                        Ambil Sendiri
                      </label>
                    </div>
                    <div class="form-check">
                      <input class="form-check-input" type="radio" name="gridRadios" id="gridRadios2" value="antar">
                      <label class="form-check-label" for="gridRadios2">
                        Di Antarkan
                      </label>
                    </div>
                  </div>
                </fieldset>
                <div class="row mb-3">
                  <label for="inputDate" class="col-sm-2 col-form-label">Date </label>
                  <div class="col-sm-10">
                    <input type="date" class="form-control" name="Waktu">
                  </div>
                </div>
                <fieldset class="row mb-3">
                  <legend class="col-form-label col-sm-2 pt-0">Waktu</legend>
                  <div class="col-sm-10">
                    <div class="form-check">
                      <input class="form-check-input" type="radio" name="gridRadiosWaktu" id="gridRadios1Waktu" value="hari" checked>
                      <label class="form-check-label" for="gridRadios1">
                        satu hari
                      </label>
                    </div>
                    <div class="form-check">
                      <input class="form-check-input" type="radio" name="gridRadiosWaktu" id="gridRadios1Waktu" value="minggu" checked>
                      <label class="form-check-label" for="gridRadios1">
                        satu minggu
                      </label>
                    </div>
                    <div class="form-check">
                      <input class="form-check-input" type="radio" name="gridRadiosWaktu" id="gridRadios1Waktu" value="bulan" checked>
                      <label class="form-check-label" for="gridRadios1">
                        satu bulan
                      </label>
                    </div>
                  </div>

                </fieldset>
                <% result.forEach((item) => { %>
                <input type="text" value="<%= item.id %>" name="iniID" style="visibility: hidden;">
                <% }) %>
                <input type="text" name="table" value="<%= tb %>" style="visibility: hidden;">
                <div class="row mb-3">
                  <label class="col-sm-2 col-form-label">Submit Button</label>
                  <div class="col-sm-10">
                    <button type="submit" class="btn btn-primary">Submit Form</button>
                  </div>
                </div>

              </form><!-- End General Form Elements -->

            </div>
          </div>

        </div>
      </div>
    </section>

  </main><!-- End #main -->

